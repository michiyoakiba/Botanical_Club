<h1>画像投稿フォーム</h1>
  <%= form_with model: @plant do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>植物名</h4>
      <%= f.text_field :plant_name %>
    <h4>説明</h4>
      <%= f.text_area :caption %>
    <%= f.submit '投稿' %>
  <% end %>
  
<p>Find me in app/views/plants/new.html.erb</p>
